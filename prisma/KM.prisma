//npx prisma migrate dev --schema=./prisma/KM.prisma --name message
//npx prisma migrate reset --schema=./prisma/KM.prisma
//npx prisma generate --schema=./prisma/KM.prisma
generator KM_client {
  provider = "prisma-client-js"
  output = "../src/generated/prisma-client/km"
}

datasource KM_db {
  provider  = "postgresql"
  url       = env("KM_DATABASE_URL")
  directUrl = env("KM_DIRECT_URL")
}

model KM_Post {
  id        String   @id @default(cuid())
  title     String
}
