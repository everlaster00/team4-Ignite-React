{
  "name": "team4-ignite-react",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "gen:main": "npx prisma generate --schema=./prisma/main/Main.prisma",
    "gen:km": "npx prisma generate --schema=./prisma/km/KM.prisma",
    "gen:nc": "npx prisma generate --schema=./prisma/nc/NC.prisma",
    "gen:jh": "npx prisma generate --schema=./prisma/jh/JH.prisma",
    "gen:pjh": "npx prisma generate --schema=./prisma/pjh/PJH.prisma",
    "gen:jw": "npx prisma generate --schema=./prisma/jw/JW.prisma",
    "gen:all": "npm run gen:main && npm run gen:km && npm run gen:nc && npm run gen:jh && npm run gen:pjh && npm run gen:jw",
    "mig:main": "npx prisma migrate dev --schema=./prisma/main/Main.prisma",
    "mig:km": "npx prisma migrate dev --schema=./prisma/km/KM.prisma",
    "mig:nc": "npx prisma migrate dev --schema=./prisma/nc/NC.prisma",
    "mig:jh": "npx prisma migrate dev --schema=./prisma/jh/JH.prisma",
    "mig:pjh": "npx prisma migrate dev --schema=./prisma/pjh/PJH.prisma",
    "mig:jw": "npx prisma migrate dev --schema=./prisma/jw/JW.prisma",
    "mig:all": "npm run mig:main && npm run mig:km && npm run mig:nc && npm run mig:jh && npm run mig:pjh && npm run mig:jw",
    "reset:main": "npx prisma migrate reset --schema=./prisma/main/Main.prisma --force",
    "reset:km": "npx prisma migrate reset --schema=./prisma/km/KM.prisma --force",
    "reset:jh": "npx prisma migrate reset --schema=./prisma/jh/JH.prisma --force",
    "reset:nc": "npx prisma migrate reset --schema=./prisma/nc/NC.prisma --force",
    "reset:pjh": "npx prisma migrate reset --schema=./prisma/pjh/PJH.prisma --force",
    "reset:jw": "npx prisma migrate reset --schema=./prisma/jw/JW.prisma --force",
    "reset:all": "npm run reset:main && npm run reset:km && npm run reset:jh && npm run reset:nc && npm run reset:pjh && npm run reset:jw",
    "deploy:main": "npx prisma migrate deploy --schema=./prisma/main/Main.prisma",
    "deploy:km": "npx prisma migrate deploy --schema=./prisma/km/KM.prisma",
    "deploy:jh": "npx prisma migrate deploy --schema=./prisma/jh/JH.prisma",
    "deploy:nc": "npx prisma migrate deploy --schema=./prisma/nc/NC.prisma",
    "deploy:pjh": "npx prisma migrate deploy --schema=./prisma/pjh/PJH.prisma",
    "deploy:jw": "npx prisma migrate deploy --schema=./prisma/jw/JW.prisma",
    "deploy:all": "npm run deploy:main && npm run deploy:km && npm run deploy:nc && npm run deploy:jh && npm run deploy:pjh && npm run deploy:jw"
  },
  "dependencies": {
    "@prisma/client": "^6.17.1",
    "@supabase/supabase-js": "^2.81.1",
    "bcrypt": "^6.0.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.553.0",
    "next": "15.5.5",
    "prisma": "^6.17.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-icons": "^5.5.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "uuid": "^13.0.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.5",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
