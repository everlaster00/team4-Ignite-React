{
  "name": "team4-ignite-react",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "gen:main": "npx prisma generate --schema=./prisma/Main.prisma",
    "gen:km": "npx prisma generate --schema=./prisma/KM.prisma",
    "gen:nc": "npx prisma generate --schema=./prisma/NC.prisma",
    "gen:jh": "npx prisma generate --schema=./prisma/JH.prisma",
    "gen:pjh": "npx prisma generate --schema=./prisma/PJH.prisma",
    "gen:jw": "npx prisma generate --schema=./prisma/JW.prisma",
    "gen:all": "npm run gen:main && npm run gen:km && npm run gen:nc && npm run gen:jh && npm run gen:pjh && npm run gen:jw",
    "mig:main": "npx prisma migrate dev --schema=./prisma/Main.prisma",
    "mig:km": "npx prisma migrate dev --schema=./prisma/KM.prisma",
    "mig:nc": "npx prisma migrate dev --schema=./prisma/NC.prisma",
    "mig:jh": "npx prisma migrate dev --schema=./prisma/JH.prisma",
    "mig:pjh": "npx prisma migrate dev --schema=./prisma/PJH.prisma",
    "mig:jw": "npx prisma migrate dev --schema=./prisma/JW.prisma"
  },
  "dependencies": {
    "@prisma/client": "^6.17.1",
    "bcrypt": "^6.0.0",
    "next": "15.5.5",
    "prisma": "^6.17.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "uuid": "^13.0.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.5",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
